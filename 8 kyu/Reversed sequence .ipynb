    "def reverse_seq(n):\n",
    "    lista = []\n",
    "    n = n\n",
    "    lista.append(n)\n",
    "    while n!=1:\n",
    "        n = n-1\n",
    "        lista.append(n)\n",
    "    return lista\n",
    "    "
